{
  "name": "template-lang",
  "displayName": "vs-template-lang",
  "description": "vuGualr template",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.81.0"
  },
  "scripts": {
    "vscode:prepublish": "cd client && npm install && cd .. && npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w",
    "lint": "eslint ./client/src ./server/src --ext .ts,.tsx",
    "postinstall": "cd client && npm install && cd ../server && npm install && cd .."
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [],
    "grammars": [
      {
          "path": "./syntaxes/inline-template.json",
          "scopeName": "inline-template.vg",
          "injectTo": [
              "source.ts"
          ],
          "embeddedLanguages": {
              "text.html": "html",
              "source.css": "css"
          }
      },
      {
        "path": "./syntaxes/template.json",
        "scopeName": "template.vg",
        "injectTo": [
          "text.html.derivative",
          "source.ts"
        ],
        "embeddedLanguages": {
          "text.html": "html",
          "source.css": "css"
        }
      }
    ]
  }
}
